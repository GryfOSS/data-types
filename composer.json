{
  "name": "gryfoss/data-types",
  "description": "PHP data types, modernised version of comely-io/data-types",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
      "name": "IDCT Bartosz Pacho≈Çek",
      "email": "bartosz+github@idct.tech",
      "role": "Developer"
    },
    {
      "name": "Furqan A. Siddiqui",
      "email": "hello@furqansiddiqui.com",
      "homepage": "https://www.furqansiddiqui.com",
      "role": "Author"
    }
  ],
  "require": {
    "php": "^8",
    "ext-bcmath": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "behat/behat": "^3.23"
  },
  "autoload": {
    "psr-4": {
      "GryfOSS\\DataTypes\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GryfOSS\\DataTypes\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage/"
  }
}
